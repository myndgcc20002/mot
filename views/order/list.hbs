<h2> Order List</h2>
<h4>
    <a href="/order" class="btn btn-success">
        <i class="fa fa-plus"> </i> Create New
    </a> 
    <a href="/product/list" class="btn btn-info"> List Product</a>
    <a href="/employee/list" class="btn btn-primary"> List Employee</a>
    <a href="/table/list" class="btn btn-warning"> List Table</a>
    <a href="/login/list" class="btn btn-danger"> Login History</a>
    <a href="/login" class="btn btn-secondary"> Logout</a>
</h4>

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Product</th>
            <th>Customer Name</th>
            <th>Quantity</th>
            <th>Total</th>
            <th>Feedback</th>
            <th>OrderDate</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.ordID}}</td>
            <td>{{this.ordProduct}}</td>
            <td>{{this.cusName}}</td>
            <td>{{this.ordQuantity}}</td>
            <td>{{this.ordTotal}}</td>
            <td>{{this.tabFeedback}}</td>
            <td>{{this.ordDate}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>